
@{
    ViewBag.Title = "Points";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
    $(document).ready(function () {
        $("#DivContent").show();
        $("#ConfContent").hide();
        $("#Super8Content").hide();
        $("#InterContent").hide();
        $("#PlayoffContent").hide();
        // Initialize tooltip
        $('[data-toggle="tooltip"]').tooltip({
            placement: 'top'
        });
        $("#btnDiv").click(function () {
            $("#DivContent").show();
            $("#ConfContent").hide();
            $("#Super8Content").hide();
            $("#InterContent").hide();
            $("#PlayoffContent").hide();
        });
        $("#btnConf").click(function () {
            $("#DivContent").hide();
            $("#ConfContent").show();
            $("#Super8Content").hide();
            $("#InterContent").hide();
            $("#PlayoffContent").hide();
        });
        $("#btnSuper8").click(function () {
            $("#DivContent").hide();
            $("#ConfContent").hide();
            $("#Super8Content").show();
            $("#InterContent").hide();
            $("#PlayoffContent").hide();
        });
        $("#btnInter").click(function () {
            $("#DivContent").hide();
            $("#ConfContent").hide();
            $("#Super8Content").hide();
            $("#InterContent").show();
            $("#PlayoffContent").hide();
        });
        $("#btnPlayoff").click(function () {
            $("#DivContent").hide();
            $("#ConfContent").hide();
            $("#Super8Content").hide();
            $("#InterContent").hide();
            $("#PlayoffContent").show();
        });
    });
</script>
<script src="~/Scripts/angular/controllers/PointsController.js"></script>
<div ng-controller="PointsCtrl">
    <h3>WMCB Championship As Of August 10, 2015</h3>
    <div class="btn-group" role="group">
        <button type="button" class="btn btn-warning" id="btnDiv">Division</button>
        <button type="button" class="btn btn-warning" id="btnConf">Conference</button>
        <button type="button" class="btn btn-warning" id="btnSuper8">Super 8</button>
        <button type="button" class="btn btn-warning" id="btnInter">Inter-Division</button>
