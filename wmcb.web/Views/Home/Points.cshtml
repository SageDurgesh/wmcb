            <h3>Division Championship </h3>
            <h4>Northern Division</h4>
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Team</th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Game Played">P</a></th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Game Won">W</a></th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Game Lost">L</a></th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Game Abandoned">D/A</a></th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Game Tied">T</a></th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Winning Percentage">Win%</a></th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Points">Points</a></th>
                        <th><a href="#" data-toggle="tooltip" data-original-title="Net Run Rate">NRR</a></th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="p in northernDivPoints">
                        <td class="col-lg-2 col-md-2">{{p.Team}}</td>
                        <td>{{p.GamePlayed}}</td>
                        <td>{{p.Win}}</td>
                        <td>{{p.Loss}}</td>
                        <td>{{p.Abandon}}</td>
                        <td>{{p.Tie}}</td>
                        <td>{{p.WinPercentage *100|number:2}}</td>
