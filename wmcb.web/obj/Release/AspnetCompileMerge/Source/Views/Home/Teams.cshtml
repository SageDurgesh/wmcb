@{
    ViewBag.Title = "Teams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Teams</h2>
<script src="~/Scripts/angular/controllers/TeamController.js"></script>
<div ng-controller="TeamCtrl">
    <table class="table table-hover col-md-5 col-lg-5">
        <thead>
            <tr><th>Team</th><th>Contact</th></tr>
        </thead>
        <tbody>
            <tr ng-repeat="team in teams">
                <td>{{team.Name}}</td>
                <td>{{team.Contact1.Name}}</td>
            </tr>
        </tbody>
    </table>

</div>





